@model IEnumerable<IEnumerable<string>>

@{
    ViewData["Title"] = $"Авточасти за нови и стари коли";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>
    @ViewData["Title"]
</h2>

<p> <b>Изберете марка</b></p>

<div class="row">
    <div class="col-lg-9 col-md-9 col-sm-12 col-xs-12">
        @foreach (var row in Model)
        {
            <div class="col-lg-3 col-md-3 col-sm-6 col-xs-6">
                <ul>
                    @foreach (var make in row)
                    {
                    var url = Url.Action("ChooseCar", "Products",
                       new
                       {
                           make = make
                       });

                        <li>
                            <a href="@url">@make</a>
                        </li>
                    }
                </ul>
            </div>
        }
    </div>
</div>